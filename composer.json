{
	"name": "orisai/tracys-pet",
	"description": "Tracy got an angry pet to remind you of your failures",
	"keywords": [
		"exception",
		"error",
		"nette",
		"orisai",
		"tracy"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/tracys-pet",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.3.0",
		"nette/di": "^3.1.0",
		"tracy/tracy": "^2.9.0"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.26.0",
		"orisai/coding-standard": "^3.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0"
	},
	"autoload": {
		"psr-4": {
			"OriNette\\TracysPet\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\OriNette\\TracysPet\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
